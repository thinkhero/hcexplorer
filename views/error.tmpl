{{define "error"}}
<!DOCTYPE html>
<html lang="en">
{{template "html-head" printf "Error %s" .ErrorCode}}
<body>

    {{template "navbar"}}

    <div class="container">
        <br/>
        <div class="alert alert-info">
            <h4>Error: {{.ErrorCode}}</h4>
        </div>
        <div class="row">
            <div class="col">
                <div class="table-parent">
                    <p>{{.ErrorString}}</p>
                </div>
            </div>
        </div>
    </div>

    {{template "footer"}}

</body>
</html>
{{end}}